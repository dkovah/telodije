// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

model Prediction {
  id         Int      @id @default(autoincrement())
  title      String
  username   String   @default("user")
  originDate DateTime
  targetDate DateTime
  body       String
  key        String
  uuid       String   @unique
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
